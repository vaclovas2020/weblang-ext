{
  "scopeName": "source.weblang",
  "fileTypes": "weblang",
  "foldingStartMarker": "\\{\\s*$",
  "foldingStopMarker": "^\\s*\\}",
  "patterns": [
    {
      "name": "support.class.weblang",
      "match": "\\b(server|controller|object|model|view|service|firewall|repository|router|cms)\\b"
    },
    {
      "name": "support.function",
      "match": "\\b(func)\\b"
    },
    {
      "name": "string.quoted.double.weblang",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.weblang",
          "match": "'\\\\."
        }
      ]
    },
    {
      "name": "comment.line.double-slash.weblang",
      "match":"\\/\\/.*"
    },
    {
      "name": "comment.block",
      "begin":"\\/\\*",
      "end": "\\*+\\/"
    },
    {
      "name":"variable.parameter",
      "begin":"@\\w+",
      "end":")"
    },
    {
      "name":"constant.numeric",
      "match":"[0-9]+"
    },
    {
      "name": "variable.other",
      "match": "[$]\\w+"
    }
  ]
}